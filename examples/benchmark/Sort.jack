class Main {

    function int main() {

        var Array a;

        let a = Main.build(1000);
        do a.print();
        do a.sort();
        do a.print();

        return 0;
    }

    function Array build(int len) {
        var Array a;
        var int i, num;

        let a = Array.new(len);
        let i = 0;
        let num = len;

        while (i < len) {
            let a[i] = num;
            let num = num - 1;
            let i = i + 1;
        }

        return a; 
    }
}
